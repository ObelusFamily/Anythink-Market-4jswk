{
	"name": "Anythink Development Container",
	"build": {
	  "dockerfile": "Dockerfile",
	  "args": { "VARIANT": "ubuntu-22.04" }
	},
  
	"forwardPorts": [3000, 3001, 5433],
	"portsAttributes": {
	  "3000": {
		"label": "Backend",
		"elevateIfNeeded": true,
		"requireLocalPort": true
	  },
	  "3001": {
		"label": "Frontend",
		"elevateIfNeeded": true,
		"requireLocalPort": true
	  },
	  "5433": {
		"label": "Database",
		"elevateIfNeeded": true,
		"requireLocalPort": true
  
	  }
	},
	"postAttachCommand": ".devcontainer/setup.sh",
  
	"features": {
	  "docker-in-docker": "latest",
	  "github-cli": "latest"
	}
  }